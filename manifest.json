{
    "name": "Woozy",
    "version": "0.0.1",
    "description": "Your drink water reminder",
    "permissions": ["alarms", "notifications"],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "128":"./assets/leaves.jpg",
        "64":"./assets/leaves.jpg",
        "32": "./assets/leaves.jpg",
        "16":"./assets/leaves.jpg"
    },
    "action": {
        "default_title": "Woozy",
        "default_popup": "popup.html"
    },
    "manifest_version": 3
}